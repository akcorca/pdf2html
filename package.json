{
  "name": "hello-cli",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Simple TypeScript CLI with Biome",
  "bin": {
    "hello-cli": "./src/cli.ts"
  },
  "scripts": {
    "start": "node ./src/cli.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "tsgo -p tsconfig.json",
    "dupcheck": "jscpd --config jscpd.json",
    "depcheck": "knip --config knip.json",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "npm run lint && npm run typecheck && npm run test:coverage && npm run dupcheck && npm run depcheck",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@vitest/coverage-v8": "^3.2.4",
    "@types/node": "^25.2.2",
    "@typescript/native-preview": "^7.0.0-dev.20260208.1",
    "husky": "^9.1.7",
    "jscpd": "^4.0.8",
    "knip": "^5.83.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "commander": "^14.0.3"
  }
}
